{
	"tags" : {
		"allowUnknownTags" : false
	} ,
	"source" : {
		"include" 			: [
			"./tests/functional/steps"
		] ,
		"includePattern" 	: ".js$" ,
		"excludePattern" 	: "(node_modules/|documentation)"
	} ,
	"plugins" : [
		"plugins/markdown"
	] ,
	"opts" : {
		"template" 		: "./node_modules/ink-docstrap/template" 			,
		"encoding" 		: "utf8" 											,
		"destination" 	: "./documentation/tests/functional/suite/steps" 	,
		"readme" 		: "./tests/functional/steps/readme.md" 				,
		"recurse" 		: true 												,
		"verbose" 		: true
	} ,
	"templates" : {
		"logoFile" 				: "" 										,
		"cleverLinks" 			: false 									,
		"monospaceLinks" 		: false 									,
		"dateFormat" 			: "ddd MMM Do YYYY" 						,
		"outputSourceFiles" 	: true 										,
		"outputSourcePath" 		: true 										,
		"systemName" 			: "Cucumber Steps" 							,
		"syntaxTheme" 			: "default" 								,
		"footer" 				: "" 										,
		"copyright" 			: "Application Copyright Â© 2012-2017." 		,
		"navType" 				: "vertical" 								,
		"theme" 				: "spacelab" 								,
		"linenums" 				: true 										,
		"collapseSymbols" 		: false 									,
		"inverseNav" 			: true 										,
		"protocol" 				: "html://" 								,
		"methodHeadingReturns" 	: false
	}
}